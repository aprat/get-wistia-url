{
  "name": "Get wistia url",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Put wistia url in a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "permissions": [
  	"tabs",
  	"<all_urls>"
  ]
}